{
  "env": {
    "dev": {
      "data": {
        "app": {
          "environment": "dev"
        }
      }
    },
    "prod": {
      "data": {
        "app": {
          "environment": "prod"
        }
      }
    }
  },
  "options": {
    "data": {
      "app": {
        "environment": "dev",
        "root": "/"
      },
      "title": "Tateru CLI Example",
      "domain": "https://www.example.com/"
    },
    "src": "example/src/twig",
    "ext": ""
  },
  "translations": {
    "cs": {
      "src": "example/src/translations/cs.json",
      "ext": ""
    }
  },
  "pages": {
    "cs": {
      "index": {
        "data": {
          "page": "index"
        },
        "src": "views/homepage.html.twig",
        "ext": "index.html",
        "minify": [
          "prod"
        ]
      },
      "about": {
        "data": {
          "page": "about"
        },
        "src": "views/about.html.twig",
        "ext": "about-us/index.html",
        "minify": [
          "prod"
        ]
      },
      "humans_txt": {
        "data": {
          "page": "humans_txt"
        },
        "src": "views/humans.txt.twig",
        "ext": "humans.txt"
      },
      "robots_txt": {
        "data": {
          "page": "robots_txt"
        },
        "src": "views/robots.txt.twig",
        "ext": "robots.txt"
      },
      "sitemap_xml": {
        "data": {
          "page": "sitemap_xml"
        },
        "src": "views/sitemap.html.twig",
        "ext": "sitemap.xml"
      },
      "webmanifest_file": {
        "data": {
          "page": "webmanifest_file"
        },
        "src": "views/site.webmanifest.twig",
        "ext": "site.webmanifest"
      }
    }
  }
}
